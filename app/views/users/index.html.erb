<div class ="container">
  <div class ="row">
    <div class ="col-md-3">
      <%= render 'books/user', user: @user %>
      <%= render 'books/book', book: @book %>
    </div>
    <div class ="col-md-8 offset-md-1">
      <h1>Users</h1>
      <div>
        <% @users.each do |user| %>
          <p>image</p>
          <%= image_tag user.get_profile_image(100,100) %>
          <p>name</p>
          <%= user.name %>
          <%= link_to "Show", user_path(user) %>
          <% end %>
      </div>
    </div>
  </div>
</div>
